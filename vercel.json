{
  "version": 2,
  "builds": [
    {
      "src": "mikaty-vitrine/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/mikaty-vitrine/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/mikaty-vitrine/build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/mikaty-vitrine/build/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/mikaty-vitrine/build/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/mikaty-vitrine/build/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/mikaty-vitrine/build/index.html"
    }
  ]
} 